# Test Environment Variables
# Copy this file to .env.test.local for testing

# Firebase Configuration (Test)
REACT_APP_FIREBASE_API_KEY=your_test_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_test_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_test_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_test_project.firebasestorage.app
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_test_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_test_app_id

# Application Configuration
REACT_APP_APP_NAME=Flight Message System (Test)
REACT_APP_APP_VERSION=1.0.0-test
REACT_APP_ENVIRONMENT=test

# Security Settings
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_CRASH_REPORTING=false

# Test Features
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=debug
REACT_APP_MOCK_FIREBASE=true
