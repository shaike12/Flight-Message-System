# Development Environment Variables
# Copy this file to .env.development.local for development builds

# Firebase Configuration (Development)
REACT_APP_FIREBASE_API_KEY=your_development_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_dev_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_dev_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_dev_project.firebasestorage.app
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_dev_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_dev_app_id

# Application Configuration
REACT_APP_APP_NAME=Flight Message System (Dev)
REACT_APP_APP_VERSION=1.0.0-dev
REACT_APP_ENVIRONMENT=development

# Security Settings
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_CRASH_REPORTING=false

# Development Features
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=debug
